{
  "name": "vitaltrack",
  "version": "1.0.0",
  "description": "Real-time health monitoring for senior care facilities.",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node src/server/index.js",
    "client": "vite --config src/client/vite.config.js",
    "build": "vite build --config src/client/vite.config.js",
    "start": "node src/server/index.js",
    "db:seed": "psql $DATABASE_URL -f src/db/seed.sql"
  },
  "keywords": [
    "health",
    "monitoring",
    "senior-care",
    "hipaa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "marked": "^17.0.1",
    "pg": "^8.16.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "recharts": "^3.5.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "vite": "^7.2.7"
  }
}
